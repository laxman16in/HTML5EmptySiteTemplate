                    <af:outputText value="BPM" styleClass="WBMetric2" id="ot1"/>
                </f:facet>
                <f:facet name="status">
                    <af:menuBar id="mb1">
                        <af:menu text="lakshmana@novartis.com" id="m2">
                            <af:commandMenuItem text="Preferences" id="cmi1"/>
                            <af:commandMenuItem text="Sign Out" id="cmi2"/>
                        </af:menu>
                    </af:menuBar>
                </f:facet>
                <f:facet name="appNav">
                    <af:button text="Home" id="b1" styleClass="WBDisplayBlockMargin AFAppNavbarButton" selected="true"
