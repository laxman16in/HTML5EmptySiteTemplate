                    <af:button text="Search" id="b2" styleClass="WBDisplayBlockMargin AFAppNavbarButton" type="radio"/>
                    <af:button text="User Management" id="b3" styleClass="WBDisplayBlockMargin AFAppNavbarButton"
                               type="radio"/>
                </f:facet>
                <f:facet name="globalLinks"></f:facet>
                <f:facet name="footer">
                    <af:navigationPane hint="bar" id="np1">
                        <af:commandNavigationItem id="ni1" text="About Novartis"/>
                        <af:commandNavigationItem id="ni2" text="Contact Us"/>
                        <af:commandNavigationItem id="ni3" text="Legal Notices"/>
                        <af:commandNavigationItem id="ni4" text="Terms of Use"/>
                        <af:commandNavigationItem id="ni5" text="Your Privacy Rights"/>
                    </af:navigationPane>
                </f:facet>
                <f:facet name="center">
                    <!--<af:link id="l1" action="MyActivities">-->
                    <af:panelGroupLayout id="pb4"
                                         styleClass="WBDisplayBlock WBDisplayBlockMargin WBDashboardCardSize78 WBDashboardCard">
                        <af:panelGroupLayout id="pgl1" layout="vertical">
                            <af:outputFormatted value="My Activities" id="of3" styleClass="WBDashboardMetricLabel"/>
                        </af:panelGroupLayout>
                        <af:panelGridLayout id="tlActs">
                            <af:gridRow id="rl1Acts">
                                <af:gridCell id="cf2Acts" width="50%" halign="stretch">
                                    <af:panelGroupLayout id="pglActs2" halign="center" layout="vertical">
                                        <af:image id="tasks" source="/resources/images/qual_tasks_48_full.png"
                                                  styleClass="WBDashboardCardIcon" shortDesc="Events Icon"/>
