                                <af:outputText id="otAppr2" value="Waiting" styleClass="WBDashboardMetricLabel2"/>
                                <af:spacer id="spAppr4" width="105" height="1"/>
                                <af:outputText id="otApprNum2" value="4" styleClass="WBDashboardLabel1232"/>
