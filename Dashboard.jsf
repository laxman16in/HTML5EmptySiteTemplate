                                <af:spacer id="spNotifyDiv2" width="1" height="1"/>
                            </af:panelGroupLayout>
                            <af:panelGroupLayout id="pglntr3" layout="horizontal">
                                <af:image id="imgAppr2" source="/resources/images/qual_reject_32_full.png"
                                          shortDesc="Reject Icon" inlineStyle="vertical-align: middle;"/>
                                <af:spacer id="spAppr5" width="10" height="1"/>
                                <af:outputText id="otAppr3" value="Rejected" styleClass="WBDashboardMetricLabel2"/>
                                <af:spacer id="spAppr6" width="100" height="1"/>
                                <af:outputText id="otApprNum3" value="1" styleClass="WBDashboardLabel12323"/>
                            </af:panelGroupLayout>
                        </af:panelGroupLayout>
                    </af:panelGroupLayout>
                    <af:spacer width="100" height="10" id="s4"/>
                    <af:panelGroupLayout id="pb10"
                                         styleClass="WBDisplayBlock WBDisplayBlockMargin WBDashboardCardSize78 WBDashboardCard">
                        <af:panelGroupLayout id="pglpt" layout="vertical">
                            <af:outputFormatted value="Attrition History" id="ofpt"
                                                styleClass="WBDashboardMetricLabel"/>
                            <af:panelGroupLayout layout="vertical" halign="center" id="pgl11">
                                <sunburst id="t2" value="" var="row" inlineStyle="width:140px;height:140px;"
                                          nodeSelection="none" summary="Attrition History Sunburst">
                                    <af:switcher facetName="Data" id="s2">
                                        <f:facet name="attritionHistory0">
                                            <sunburstNode value="3" id="sn2">
                                                <attributeGroups value="1" type="color" id="ag2"/>
                                            </sunburstNode>
                                        </f:facet>
                                    </af:switcher>
                                </sunburst>
                            </af:panelGroupLayout>
                        </af:panelGroupLayout>
                    </af:panelGroupLayout>
                    <!--lsa remove style class when tablet first template fixed-->
                    <!--<af:region value="#{bindings.dynamicRegion1.regionModel}" id="r1"
                               partialTriggers=":::bBarFDC:b1 :::bBarFDC:b2 :::bBarFDC:b3"
                               styleClass="ContentContainerWorkaround"/>-->
                </f:facet>
                <f:facet name="start"/>
                <f:facet name="end"/>
                <f:attribute name="endWidth" value="0px"/>
                <f:attribute name="startWidth" value="0px"/>
                <f:attribute name="showFooter" value="true"/>
            </af:pageTemplate>
