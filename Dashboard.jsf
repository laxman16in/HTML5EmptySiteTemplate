                        </af:panelGroupLayout>
                        <af:panelGroupLayout id="pglntc" layout="vertical">
                            <af:panelGroupLayout id="pglntr1" layout="horizontal">
                                <af:image id="imgAppr" source="/resources/images/qual_approved_32_full.png"
