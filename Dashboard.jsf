                                <af:outputText id="otAppr" value="Approved" styleClass="WBDashboardMetricLabel2"/>
                                <af:spacer id="spAppr2" width="95" height="1"/>
                                <af:outputText id="otApprNum" value="3" styleClass="WBDashboardLabel123"/>
