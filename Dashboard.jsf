                                    </af:panelGroupLayout>
                                </af:gridCell>
                            </af:gridRow>
                            <af:gridRow id="rl2Acts" marginTop="10px">
                                <af:gridCell id="cf4Acts" halign="center">
                                    <af:outputText id="ot2Acts" value="Tasks" styleClass="WBDashboardLabel"/>
                                </af:gridCell>
                            </af:gridRow>
                        </af:panelGridLayout>
                    </af:panelGroupLayout>
                    <af:spacer width="100" height="10" id="s3"/>
                    <af:panelGroupLayout id="pb3"
                                         styleClass="WBDisplayBlock WBDisplayBlockMargin WBDashboardCardSize782 WBDashboardCard">
                        <af:panelGroupLayout id="pgl6" layout="vertical">
                            <af:outputFormatted value="My Notifications" id="of1" styleClass="WBDashboardMetricLabel"/>
